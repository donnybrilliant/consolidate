{
  "name": "consolidate",
  "displayName": "consolidate",
  "publisher": "Vierweb",
  "description": "A VS Code extension to consolidate all files into their paths and contents.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/donnybrilliant/consolidate.git"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "consolidate.consolidateFiles",
        "title": "Consolidate Files"
      },
      {
        "command": "consolidate.consolidateSelectedFiles",
        "title": "Consolidate Selected Files"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "consolidate.consolidateSelectedFiles",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "ignore": "^5.3.2"
  }
}
